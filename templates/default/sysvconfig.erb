#
# System configuration for Selenium
#
# Generated by Chef for <%= node[:fqdn] %>
#
SELENIUM_USER=<%= node['selenium']['server']['user'] %>
SELENIUM_JAR=<%= File.join(node['selenium']['server']['installpath'], 'selenium-server-standalone.jar') %>
SELENIUM_ROLE=<%= @role %>
SELENIUM_LOG=<%= @log %>
SELENIUM_XMX=<%= @xmx %>
<% if @role == 'node' %>
SELENIUM_NODE_CONFIG=<%= @config %>
SELENIUM_FBSIZE=<%= @fbsize %>
<% end %>
